{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "alias": {
          "@common": "./src/common",
          "@hooks": "./src/hooks",
          "@table": "./src/Table",
          "@theme": "./src/Theme",
          "@sort": "./src/Sort",
          "@select": "./src/Select",
          "@tree": "./src/Tree",
          "@resize": "./src/Resize",
          "@expand": "./src/Expand"
        }
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  }
}
