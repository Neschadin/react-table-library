  "directories": {
    "lib": "lib"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1",
