{
  "name": "@table-library/react-table-library",
  "version": "0.0.77",
  "description": "react-table-library",
  "main": "lib/main.js",
  "peerDependencies": {
    "react": "^16.13.1 || ^17",
    "react-dom": "^16.13.1 || ^17"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.isequal": "^4.5.0",
    "styled-components": "^5.0.1",
    "use-double-click": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "7.12.17",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "7.12.17",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-react": "7.12.13",
    "@babel/runtime": "7.12.18",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@storybook/addon-docs": "6.0.22",
    "@storybook/cli": "6.0.22",
    "@storybook/react": "6.0.22",
    "@storybook/source-loader": "6.0.22",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "core-js": "3.9.0",
    "css-loader": "3.2.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^5.0.9",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "loki": "0.25.1",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "1.1.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "scripts": {
    "prettier": "prettier 'src/**/*.{js,ts,tsx}'",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --ignore-pattern .eslintignore --max-warnings 0",
    "lint:check": "npm run lint",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --config ./.storybook/jest.config.json",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm run test -- --u",
    "loki:test": "loki test",
    "loki:update": "loki update",
    "storybook:dev": "start-storybook --docs -c .storybook -s .storybook/static -p 9001",
    "storybook:build": "build-storybook --docs -c .storybook -s .storybook/static -o dist",
    "library:build": "rollup -c && npm run copypackage",
    "check": "npm run test && npm run prettier:check && npm run lint:check",
    "copypackage": "cp -rf package.json lib",
    "dist": "npm run check && npm run library:build && cd lib && npm publish",
    "prepublishOnly": "echo \"Warning: Don't run 'npm publish' in root. Use 'npm run dist' instead.\" && exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier:write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/table-library/react-table-library.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "datagrid",
    "react-table",
    "react-datagrid",
    "react-data-grid",
    "react-data-table",
    "react-datatable",
    "react-table-library"
  ],
  "author": "Robin Wieruch <hello@rwieruch.com> (https://www.robinwieruch.de)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/table-library/react-table-library/issues"
  },
  "homepage": "https://github.com/table-library/react-table-library#readme"
}
