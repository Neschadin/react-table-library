{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "alias": {
          "@table-library/react-table-library/lib/common": "./src/common",
          "@table-library/react-table-library/lib/table": "./src/table",
          "@table-library/react-table-library/lib/theme": "./src/theme",
          "@table-library/react-table-library/lib/resize": "./src/resize",
          "@table-library/react-table-library/lib/panel": "./src/panel",
          "@table-library/react-table-library/lib/sort": "./src/sort",
          "@table-library/react-table-library/lib/select": "./src/select",
          "@table-library/react-table-library/lib/tree": "./src/tree"
        }
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  }
}
